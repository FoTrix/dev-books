---
import { PUBLIC_REPO_API } from "astro:env/server";
import axios from "axios";




    const response = await axios.get(PUBLIC_REPO_API);
    const repos =  await response.data;

---


{
    repos.map((repo: {name: string, url: string}) => (
        <h1> name: {repo.name} </h1>
        <p> url: { repo.url }</p>
    ))
}